<div class='question-container post-container'>
  <%= erb :'votes/_vote', :locals => { :voteable => question } %>
  <div class='postbar'>
    <h3><a href="/questions/<%= question.id %>"><%= html_escaper(question.title) %></a></h3>
    <p>by <%= html_escaper(question.author.username) %></p>
  </div>
</div>
